<h1>Editing horario</h1>

<% form_for(@horario) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :horas %><br />
    <%= f.text_field :horas %>
  </p>
  <p>
    <%= f.label :cubiculo %><br />
    <%= f.text_field :cubiculo %>
  </p>
  <p>
    <%= f.label :cita_id %><br />
    <%= f.text_field :cita_id %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @horario %> |
<%= link_to 'Back', horarios_path %>
