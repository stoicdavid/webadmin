<h1>Editing search</h1>

<% form_for(@search) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :nombre %><br />
    <%= f.text_field :nombre %>
  </p>
  <p>
    <%= f.label :nombre_2 %><br />
    <%= f.text_field :nombre_2 %>
  </p>
  <p>
    <%= f.label :ap_pat %><br />
    <%= f.text_field :ap_pat %>
  </p>
  <p>
    <%= f.label :ap_mat %><br />
    <%= f.text_field :ap_mat %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @search %> |
<%= link_to 'Back', searches_path %>
